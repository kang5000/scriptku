function jp()
gg.setRanges(gg.REGION_ANONYMOUS | gg.REGION_C_BSS)
gg.clearResults()
gg.searchNumber("10F;1F;1108344832D:29", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.processResume()
revert = gg.getResults(5000, nil, nil, nil, nil, nil, nil, nil, nil)
gg.editAll("2", gg.TYPE_FLOAT)
revert = gg.getResults(5000, nil, nil, nil, nil, nil, nil, nil, nil)
gg.editAll("1120403456", gg.TYPE_DWORD)
gg.processResume()
end


function highlight()
gg.loadList(gg.EXT_STORAGE .. "/Android/data/com.tencent/bluecolor.txt", 0)
t = gg.getListItems()
gg.loadResults(t)
gg.getResults(1000)
gg.editAll("-99999", 4)
C = gg.getListItems(P)
gg.removeListItems(C)
gg.clearResults()
end















    function Exit()
      print("‚ùè s·¥Ä ü·¥Ä·¥ç ·¥Ñ…™·¥õ·¥á Ä ·¥ã·¥Ä·¥ç·¥ò Ä·¥á·¥õ ‚ùè ")
gg.skipRestoreState()
gg.setVisible(true)
os.exit()
end



while true do
  if gg.isVisible(true) then
    XGCK = 1
    gg.setVisible(false)
  end
  gg.clearResults()
  if XGCK == 1 then
    Main() end
    end
