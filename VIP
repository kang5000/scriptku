function X5()
  gg.setRanges(gg.REGION_ANONYMOUS)
  gg.searchNumber("34", gg.TYPE_FLOAT)
  gg.refineNumber("34", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
  gg.getResults(1000)
  gg.editAll("10", gg.TYPE_FLOAT)
  gg.clearResults(x)
  gg.setRanges(gg.REGION_ANONYMOUS)
  gg.searchNumber("34", gg.TYPE_FLOAT)
  gg.refineNumber("34", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
  gg.getResults(1000)
  gg.editAll("5", gg.TYPE_FLOAT)
  gg.toast("üî≠ VIEW SCOPE X6")
  gg.clearResults(x)
end

